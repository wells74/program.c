//Program to print table of a number using functions.

#include<stdio.h>
main()
{
int a[50],b[50],c[50],n,i,j,k;
printf("how many number you want to store in a single array\n");
scanf("%d",&n);
printf("now enter %d elements of the first array one by one by one\n",n);
for(int i=1;i<=n;i++)
scanf("%d",&a[i]);
printf("now enter the element of the second array one by one\n",n);
for(int j=1;j<=n;j++)
scanf("%d",&b[j]);
printf("addition of the first and second array is given below\n");
for(int k=1;k<n;k++)
{
c[k]=a[k]+b[k];
printf("%d\n",c[k]);
}
}
